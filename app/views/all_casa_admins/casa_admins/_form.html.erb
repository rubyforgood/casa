<%= form_for casa_admin, url: all_casa_admins_casa_org_casa_admins_path(casa_org), id: :new_casa_admin do |form| %>
  <%= render "/shared/error_messages", resource: casa_admin %>
  <div class="input-style-1">
    <%= form.label :email, "Email" %>
    <%= form.text_field :email, class: "form-control" %>
  </div>
  <div class="input-style-1">
    <%= form.label :display_name, "Display name" %>
    <%= form.text_field :display_name, class: "form-control" %>
  </div>
  <div class="actions">
    <%= button_tag( type: "submit" , class: "btn-sm main-btn primary-btn btn-hover" ) do %>
      <i class="lni lni-checkmark-circle mr-5"></i> Submit
    <% end %>
  </div>
<% end %>
